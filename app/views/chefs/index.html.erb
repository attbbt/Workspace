<%= render 'shared/page_title', title: "Chefs" %>

<div class="row">
  <div class="col-md-4 col-md-offset-4 center">
    <%= will_paginate %>
  </div>
</div>

<% @chefs.each do |chef| %>
  <div class="row">
    <div class="col-md-4 col-md-offset-4 center">
     <%= link_to image_tag(chef.picture? ? chef.picture.url :  'missing.png', size:"200", class: 'gravatar'), chef_path(chef) %>
    </div>
  </div>
<%end %>